                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH cte AS
(
	SELECT 		
		drf.[BatchId],
		[LineItemId] = row_number() OVER (PARTITION BY drf.[BatchId], drf.[DateId], drf.[LocationNumber] ORDER BY drf.[DateId], drf.[LocationNumber], drf.[RoyaltyGroupId], drf.[PlanId]),
		[DateId] = CONVERT(VARCHAR(8), DATEADD(DAY, -1, SUBSTRING(CAST(drf.[DateId] AS CHAR(8)),1,4) + '-' + SUBSTRING(CAST(drf.[DateId] AS CHAR(8)),5,2) + '-' + SUBSTRING(CAST(drf.[DateId] AS CHAR(8)),7,2)),112),
        drf.LocationNumber, 		
		drf.RoyaltyGroupId,
        drf.RoyaltyGroupName, 
        drf.PlanId, 
        drf.PlanName, 
        drf.[TaxableAmount], 
        drf.[NonTaxableAmount], 
        drf.[ShippingAmount], 
        drf.[PostageAmount], 
        drf.[DiscountAmount], 
        drf.[TaxAmount], 
        drf.[RevenueAmount], 
        drf.[RoyaltyAmount], 
        drf.[SubTotalAmount], 
        drf.[TotalSalesAmount], 
        drf.[Currency]		
    FROM [fs-dw-stg].[CoreBridge].[DailyRoyaltyFileV2] drf
	JOIN [fs-dw-stg].[dbo].[File] f on drf.[FileId] = f.[FileId] AND f.[FileType] = 1     
	WHERE drf.[BatchId] = ?
)

SELECT *
FROM cte
ORDER BY DateId, LocationNumber</property>
